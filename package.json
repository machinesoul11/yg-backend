{
  "name": "yg-backend",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "prisma generate && next build",
    "start": "next start",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "postinstall": "prisma generate",
    "format": "prettier --write .",
    "format:check": "prettier --check .",
    "type-check": "tsc --noEmit",
    "db:generate": "prisma generate",
    "db:push": "prisma db push",
    "db:migrate": "prisma migrate dev",
    "db:migrate:deploy": "prisma migrate deploy",
    "db:migrate:status": "prisma migrate status",
    "db:migrate:resolve": "prisma migrate resolve",
    "db:studio": "prisma studio",
    "db:seed": "tsx prisma/seed.ts",
    "db:health": "tsx src/scripts/db-health-check.ts",
    "db:health-check": "tsx src/scripts/db-health-check.ts",
    "db:backup:verify": "tsx src/scripts/verify-backup.ts",
    "db:analyze:indexes": "tsx src/scripts/db-analyze-indexes.ts",
    "db:analyze:vacuum": "tsx src/scripts/db-analyze-vacuum.ts",
    "db:check-bloat": "tsx src/scripts/db-analyze-vacuum.ts",
    "email:dev": "email dev",
    "email:build": "email build",
    "clean:rf": "node scripts/cleanup-resource-forks.js --watch",
    "clean:rf:once": "node scripts/cleanup-resource-forks.js --clean-only",
    "cache:init": "tsx src/scripts/initialize-cache.ts",
    "cache:warm": "tsx src/scripts/initialize-cache.ts",
    "cache:health": "tsx -e \"import { redisMonitor } from './src/lib/redis/monitoring'; redisMonitor.getHealthStatus().then(h => console.log(h))\"",
    "cache:metrics": "tsx -e \"import { cachePerformanceService } from './src/lib/redis/cache-performance.service'; cachePerformanceService.getCurrentMetrics().then(m => console.log(m))\""
  },
